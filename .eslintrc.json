{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-multiple-empty-lines": [2, {"max": 1, "maxEOF": 0}],
        "no-unused-vars": "warn",
        "no-console": "warn",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off"
    }
}
